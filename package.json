{
    "name": "swrap",
    "description": "Simple Node service wrapper, with basic support for configs and classes.",
    "version": "0.0.4",
    "author": "Gustavo Henke <gustavo@injoin.com.br>",
    "keywords": [
        "app",
        "service",
        "wrapper",
        "wrap",
        "config",
        "cjson",
        "class",
        "oop"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com/injoin/swrap.git"
    },
    "dependencies": {
        "cjson": "0.2.1",
        "dotty": "0.0.2",
