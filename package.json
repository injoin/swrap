        "eventemitter2": "~0.4.13"
    },
    "devDependencies": {
        "chai": "~1.7.2",
        "mocha": "~1.12.0",
        "jscoverage": "https://github.com/gustavohenke/jscoverage/tarball/master",
        "sinon": "~1.7.3",
        "mocha-lcov-reporter": "0.0.1",
        "coveralls": "~2.1.0"
    },
    "scripts": {
        "prepublish": "npm prune",
